{
  "manifest_version": 3,
  "name": "Crawler Manager Extension",
  "version": "1.0",
  "permissions": ["activeTab", "scripting", "storage", "offscreen"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["<all_urls>"]
}
